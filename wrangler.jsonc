{
	"$schema": "node_modules/wrangler/config-schema.json",
	"main": "dist/_worker.js/index.js",
	"name": "thediscdb",
	"compatibility_date": "2025-11-09",
	"compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
		"enabled": true,
		"logs": {
			"enabled": true
		},
		"traces": {
			"enabled": true
		}
	},
	"routes": [
		{
			"custom_domain": true,
			"pattern": "thediscdb.nuro.dev"
		}
	]
}
